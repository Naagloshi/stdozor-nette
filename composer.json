{
	"name": "stdozor/nette",
	"description": "StavebnÃ­ dozor - Nette verze",
	"type": "project",
	"license": "proprietary",
	"require": {
		"php": ">= 8.3",
		"nette/application": "^3.2",
		"nette/bootstrap": "^3.2",
		"nette/caching": "^3.3",
		"nette/database": "^3.2",
		"nette/di": "^3.2",
		"nette/forms": "^3.2",
		"nette/http": "^3.2",
		"nette/mail": "^4.0",
		"nette/robot-loader": "^4.0",
		"nette/routing": "^3.1",
		"nette/security": "^3.2",
		"nette/utils": "^4.0",
		"latte/latte": "^3.0",
		"tracy/tracy": "^2.10",
		"contributte/translation": "^2.0",
		"spomky-labs/otphp": "^11.4",
		"endroid/qr-code": "^6.0",
		"web-auth/webauthn-lib": "^5.2"
	},
	"require-dev": {
		"nette/tester": "^2.5",
		"phpstan/phpstan": "^2.0",
		"mockery/mockery": "^1.6",
		"phpstan/phpstan-nette": "^2.0",
		"friendsofphp/php-cs-fixer": "^3.94"
	},
	"autoload": {
		"psr-4": {
			"App\\": "app/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\": "tests/"
		}
	},
	"scripts": {
		"tester": "vendor/bin/tester tests -s -C",
		"phpstan": "vendor/bin/phpstan analyse",
		"cs-check": "vendor/bin/php-cs-fixer fix --dry-run --diff",
		"cs-fix": "vendor/bin/php-cs-fixer fix"
	},
	"minimum-stability": "stable",
	"config": {
		"allow-plugins": {
			"dealerdirect/phpcodesniffer-composer-installer": true
		}
	}
}
